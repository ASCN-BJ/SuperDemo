<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomViewBase">
        <attr name="TitleText" format="string" />
        <attr name="TitleTextColor" format="color" />
        <attr name="TitleTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CustomViewBase2">
        <attr name="TitleTexts" format="string" />

        <attr name="TitleTextss" format="string" />
        <attr name="TitleTextColors" format="color" />
        <attr name="TitleTextSizes" format="dimension" />
        <attr name="ImageSrcs" format="reference" />
        <attr name="imageScaleTypes" format="enum">
            <enum name="fitXys" value="0" />
            <enum name="centers" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="SmartRatingStar">
        <attr name="FirstDrawable" format="reference" />
        <attr name="SecondDrawable" format="reference" />
        <attr name="MaxStarNumber" format="integer" />
        <attr name="MiniStarNumber" format="integer" />
        <attr name="StarMargin" format="integer" />
        <attr name="TotalStars" format="integer" />
    </declare-styleable>
    <declare-styleable name="MultiTitleLayout">
        <attr name="view_width" format="dimension" />
        <attr name="view_heigh" format="dimension" />
    </declare-styleable>
</resources>